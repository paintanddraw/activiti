  <process id="LeaveProcess" name="请假流程" isExecutable="true">
    <startEvent id="start" name="开始"></startEvent>
    <userTask id="leave_apply" name="填写请假申请">
      <extensionElements>
        <activiti:formProperty id="message" name="请假的信息" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="submitTime" name="提交时间" type="date" datePattern="yyyy-MM-dd" required="true"></activiti:formProperty>
        <activiti:formProperty id="name" name="请假人" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="submitType" name="提交类型" type="string" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="start" targetRef="leave_apply"></sequenceFlow>
    <exclusiveGateway id="submitOrCancel" name="提交或取消"></exclusiveGateway>
    <sequenceFlow id="flow2" sourceRef="leave_apply" targetRef="submitOrCancel"></sequenceFlow>
    <userTask id="approve1" name="直接主管审批">
      <extensionElements>
        <activiti:formProperty id="approval1" name="主管审批结果" type="string"></activiti:formProperty>
        <activiti:formProperty id="message" name="主管备注" type="string"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow3" sourceRef="submitOrCancel" targetRef="approve1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${submitType=="y" || submitType=="Y"}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow4" sourceRef="approve1" targetRef="exclusivegateway2"></sequenceFlow>
    <userTask id="approve2" name="部门主管审批">
      <extensionElements>
        <activiti:formProperty id="approval2" name="部门主管审批结果" type="string"></activiti:formProperty>
        <activiti:formProperty id="message" name="部门主管备注" type="string"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow5" sourceRef="exclusivegateway2" targetRef="approve2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approva1=="y" || aproval=="Y"}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow6" sourceRef="approve2" targetRef="exclusivegateway3"></sequenceFlow>
    <endEvent id="end" name="结束"></endEvent>
    <sequenceFlow id="flow7" sourceRef="exclusivegateway3" targetRef="end">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approval2=="y" || approval2=="Y"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow12" sourceRef="exclusivegateway2" targetRef="leave_apply">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approva1=="n" || aproval=="N"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="exclusivegateway3" targetRef="leave_apply">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approval2=="n" || approval2=="N"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow14" sourceRef="submitOrCancel" targetRef="end"></sequenceFlow>
  </process>
